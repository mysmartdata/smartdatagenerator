{
    "name": "flighthub/smartdata",
    "type": "library",
    "description": "PHP library top manage data about the world.",
    "keywords": ["World", "Data", "Smart"],
    "homepage": "https://github.com/flighthub/smartdata",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Bull",
            "email": "me@gabrielbull.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "2.*",
        "psr/log": "~1",
        "maxmind-db/reader": "0.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
        "psr-4": {
            "Flighthub\\SmartData\\": "src/SmartData"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flighthub\\SmartData\\Tests\\": ["tests/SmartData/Tests", "tests/SmartData/Tests/_includes"]
        }
    }
}